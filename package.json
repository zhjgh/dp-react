{
  "name": "dp-react",
  "version": "0.1.0",
  "description": "基于antd4.x封装的组件",
  "keywords": [
    "SearchGroup",
    "Table"
  ],
  "homepage": "https://github.com/zhjgh/dp-react/",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhjgh/dp-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhjgh/dp-react.git"
  },
  "contributors": [
    "zhanghj"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "father:dev": "npx --max_old_space_size=8096 father doc dev --host 0.0.0.0",
    "father:build": "father build",
    "father:build:doc": "father doc build",
    "father:deploy": "father doc deploy"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "@babel/runtime": "7.7.1",
    "antd": "4.8.5",
    "father": "^2.30.2",
    "lodash": "^4.17.20",
    "lodash-es": "^4.17.20"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "babel-plugin-import": "^1.13.3",
    "husky": "^4.3.8",
    "mockjs": "^1.1.0",
    "postcss": "8.0.0",
    "postcss-modules": "^4.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "browserslist": [
    "last 2 version",
    "Firefox ESR",
    "> 1%",
    "ie >= 9"
  ],
  "engines": {
    "node": ">=10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "react-hooks/exhaustive-deps": "off"
  },
  "author": "zhanghj",
  "size-limit": [
    {
      "path": "dist/dp-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/dp-react.esm.js",
      "limit": "10 KB"
    }
  ]
}
