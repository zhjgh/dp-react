EAAhB;AAAoBK,MAAAA,SAAS,EAAE;AAA/B,KAA3B;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBACE;AAAQ,IAAA,IAAI,EAAC,SAAb;AAAuB,IAAA,OAAO,EAAE7B,QAAhC;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACG/B,OAAO,CAACe,IAAR,CAAa8C,MADhB,CADF,EAIGpD,MAAM,CAACqD,MAAP,GAAgBzD,MAAhB,gBACC;AAAQ,IAAA,KAAK,EAAE;AAAE0D,MAAAA,UAAU,EAAE;AAAd,KAAf;AAAkC,IAAA,OAAO,EAAEjC,cAA3C;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACGD,WAAW,GACR7B,OAAO,CAACe,IAAR,CAAaiD,UADL,GAERhE,OAAO,CAACe,IAAR,CAAakD,UAHnB,CADD,GAMG,IAVN,eAWE;AAAQ,IAAA,KAAK,EAAE;AAAEF,MAAAA,UAAU,EAAE;AAAd,KAAf;AAAkC,IAAA,OAAO,EAAEf,OAA3C;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,KACGhD,OAAO,CAACe,IAAR,CAAamD,KADhB,CAXF,CADF,CAhCF,CADF,CADF;AAsDD,CAzID;;cAAMjE,W;;AA2INA,WAAW,CAACkE,YAAZ,GAA2B;AACzB7D,EAAAA,OAAO,EAAE,CADgB;AAEzBD,EAAAA,MAAM,EAAE;AAFiB,CAA3B;eAKeJ,W;AAAf;;;;;;;;;;;;;;;;;;;;;;0BAhJMA,W","sourcesContent":["// @ts-nocheck\r\nimport React, { useMemo, useReducer, useEffect } from 'react'\r\nimport { Card, Form, Row, Col, Button } from 'antd'\r\nimport { SearchGroupProps, LabelType, SelectField } from '.'\r\nimport Field from '../Field'\r\nimport { pick } from 'lodash'\r\nimport setting from './setting'\r\n\r\nconst SearchGroup: React.FC<SearchGroupProps> = props => {\r\n  const [form] = Form.useForm()\r\n  const { maxNum, colSpan, onSearch, extraParams } = props\r\n\r\n  const fields = useMemo(\r\n    () =>\r\n      props.fields.map(item => ({\r\n        ...item,\r\n        type: item.type || 'input',\r\n        componentProps: {\r\n          ...(item.componentProps || {}),\r\n          placeholder:\r\n            item.placeholder ||\r\n            (item.type === 'rangePicker' && [\r\n              setting.text.startTime,\r\n              setting.text.endTime,\r\n            ]) ||\r\n            (item.type === 'select' && setting.text.pleaseSelect) ||\r\n            `${setting.text.pleaseInput}${item.name}`,\r\n          options:\r\n            Object.prototype.toString.call((item as SelectField).options) ===\r\n            '[object Array]'\r\n              ? (item as SelectField).options\r\n              : Object.prototype.toString.call(\r\n                  (item as SelectField).options\r\n                ) === '[object Object]'\r\n              ? Object.entries(\r\n                  (item as SelectField).options\r\n                ).map(([name, value]) => ({ name, value }))\r\n              : [],\r\n        },\r\n      })),\r\n    [props.fields]\r\n  )\r\n  const [isCollapsed, toggleCollapse] = useReducer(state => !state, true)\r\n\r\n  // 查询\r\n  const onSubmit = () => {\r\n    form\r\n      .validateFields()\r\n      .then(values => {\r\n        onSearch({\r\n          ...(extraParams || {}),\r\n          ...fields\r\n            .filter(v => v.type === 'rangePicker')\r\n            .map(v => v.label)\r\n            .reduce(\r\n              (acc, [start, end]) => ({\r\n                ...acc,\r\n                [start]:\r\n                  values[start] && values[start][0]\r\n                    ? values[start][0].format('YYYY-MM-DD')\r\n                    : undefined,\r\n                [end]:\r\n                  values[start] && values[start][1]\r\n                    ? values[start][1].format('YYYY-MM-DD')\r\n                    : undefined,\r\n              }),\r\n              values\r\n            ),\r\n        })\r\n      })\r\n      .catch(error => {\r\n        console.log(error)\r\n      })\r\n  }\r\n\r\n  // 清空\r\n  const onClean = () => {\r\n    form.resetFields()\r\n    onSubmit()\r\n  }\r\n\r\n  useEffect(() => {\r\n    console.log(Object.prototype.toString.call(extraParams))\r\n    if (Object.prototype.toString.call(extraParams) === '[object Object]') {\r\n      form.setFieldsValue(\r\n        pick(extraParams, Object.keys(form.getFieldsValue() || {}))\r\n      )\r\n    }\r\n    onSubmit()\r\n  }, [])\r\n\r\n  return (\r\n    <Card>\r\n      <Form form={form}>\r\n        <Row gutter={[0, 10]} align=\"middle\" style={{ marginBottom: 10 }}>\r\n          {fields.map((v, i) => (\r\n            <Col\r\n              key={\r\n                typeof (v.label as LabelType) === 'string'\r\n                  ? (v.label as string)\r\n                  : v.label[0]\r\n              }\r\n              span={isCollapsed && i >= maxNum ? 0 : colSpan}\r\n              style={{ paddingRight: 16 }}\r\n            >\r\n              <Form.Item\r\n                label={v.name}\r\n                name={\r\n                  typeof (v.label as LabelType) === 'string'\r\n                    ? (v.label as string)\r\n                    : v.label[0]\r\n                }\r\n                style={{ width: '100%' }}\r\n                initialValue={v.initialValue}\r\n                {...setting.layout.formItemLayout}\r\n              >\r\n                <Field\r\n                  type={v.type}\r\n                  componentProps={v.componentProps}\r\n                />\r\n              </Form.Item>\r\n            </Col>\r\n          ))}\r\n        </Row>\r\n\r\n        <Row justify=\"end\" align=\"middle\">\r\n          <Col span={colSpan} style={{ paddingRight: 16, textAlign: 'right' }}>\r\n            <Button type=\"primary\" onClick={onSubmit}>\r\n              {setting.text.search}\r\n            </Button>\r\n            {fields.length > maxNum ? (\r\n              <Button style={{ marginLeft: 8 }} onClick={toggleCollapse}>\r\n                {isCollapsed\r\n                  ? setting.text.moreSearch\r\n                  : setting.text.hideSearch}\r\n              </Button>\r\n            ) : null}\r\n            <Button style={{ marginLeft: 8 }} onClick={onClean}>\r\n              {setting.text.clean}\r\n            </Button>\r\n          </Col>\r\n        </Row>\r\n      </Form>\r\n    </Card>\r\n  )\r\n}\r\n\r\nSearchGroup.defaultProps = {\r\n  colSpan: 6,\r\n  maxNum: 8,\r\n}\r\n\r\nexport default SearchGroup\r\n"]}]}